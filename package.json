{
  "name": "typexpress",
  "version": "0.1.0",
  "description": "TypExpress",
  "main": "App.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "watch": "tsc --watch",
    "dev": "nodemon ./dist/App.js",
    "start": "node ./dist/App.js",
    "tslint": "tslint -c ./tslint.json --format stylish ./src/**/*.ts",
    "tslint-fix": "tslint -c ./tslint.json --fix ./src/**/*.ts",
    "migration:create": "./node_modules/.bin/typeorm migration:create",
    "migration:run": "./node_modules/.bin/typeorm migration:run",
    "migration:revert": "./node_modules/.bin/typeorm migration:revert"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/defudef/typexpress.git"
  },
  "keywords": [
    "typescript",
    "express",
    "typeorm"
  ],
  "author": "Marcin Drwiega",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/defudef/typexpress/issues"
  },
  "homepage": "https://github.com/defudef/typexpress#readme",
  "dependencies": {
    "@sendgrid/mail": "^6.3.1",
    "bcrypt": "^3.0.1",
    "body-parser": "^1.18.3",
    "class-transformer": "^0.1.9",
    "class-validator": "^0.14.0",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "pg": "^7.4.3",
    "reflect-metadata": "^0.1.12",
    "routing-controllers": "^0.7.7",
    "typeorm": "^0.2.7",
    "typescript": "^2.9.2",
    "uuid": "^3.3.2",
    "yarn": "^1.9.4"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.7.1",
    "@types/oauth2orize": "^1.8.3",
    "@types/passport": "^0.4.6",
    "@types/uuid": "^3.4.3",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.3.1"
  }
}
